<form method="post" action="{{ path('app_annonce_delete', {'slug': annonce.slug}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette annonce?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ annonce.slug) }}">
    <button class="btn">Supprimer</button>
</form>
